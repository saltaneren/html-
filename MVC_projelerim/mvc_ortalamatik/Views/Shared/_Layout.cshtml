<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OrtalaMatik</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/OrtalaMatik.styles.css" asp-append-version="true" />
</head>
<div class="renk  bg-secondary"   >
<body   >
    <header>
        <nav class="navbar navbar-light bg-warning">
            <a class="navbar-brand" style="margin-left: 400px;" href="#">OrtalaMatik</a>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            <br>
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - OrtalaMatik - <a asp-area="" asp-controller="Home" asp-action="Privacy">gizlilik</a>
        </div>
    </footer>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
</body>
</div>
</html>
